    "import numpy as np\n",
    "\n",
    "z = np.array([2, 1, 0.5])\n",
    "\n",
    "def softmax(x):\n",
    "    return np.exp(x) / np.exp(x).sum()\n",
    "\n",
    "print(softmax(z))\n",
    "\n",
    "softmax(z).sum()"
